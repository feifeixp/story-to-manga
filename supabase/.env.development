# Application Configuration
APP_URL=http://localhost:8000
FRONTEND_URL=http://localhost:8000
PRODUCTION_URL=
APP_NAME=Story to Manga
APP_VERSION=2.4.0
APP_ENVIRONMENT=development

# Feature Flags
ENABLE_AI_GENERATION=true
ENABLE_SHARING=true
ENABLE_COMMENTS=true
